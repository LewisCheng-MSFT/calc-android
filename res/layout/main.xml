<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="#111111"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:weightSum="1">
<EditText
	android:layout_width="match_parent"
	android:id="@+id/etInput"
	android:layout_height="wrap_content"
	android:layout_weight="0.05"
	android:textSize="16pt"
	android:textColor="#FFFFFF"
	android:editable="false"
	android:scrollHorizontally="true"
	android:background="#000000">
    <requestFocus>
    </requestFocus>
</EditText>
<TextView
	android:id="@+id/tvResult"
	android:layout_width="fill_parent"
	android:text="TextView"
	android:gravity="center_vertical|right"
	android:textSize="12pt"
	android:layout_weight="0.05"
	android:layout_height="wrap_content">
</TextView>
<TableLayout
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:id="@+id/tableLayout1"
	android:layout_weight="0.8"
	android:weightSum="1">
    <TableRow android:id="@+id/tableRow1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="0.166" android:weightSum="1">
        <Button android:text="^" android:textSize="12pt" android:id="@+id/btnPower" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="!" android:textSize="12pt" android:id="@+id/btnFact" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="%" android:textSize="12pt" android:id="@+id/btnMod" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="C" android:textSize="12pt" android:id="@+id/btnClear" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
    </TableRow>
    <TableRow android:id="@+id/tableRow2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="0.166" android:weightSum="1">
        <Button android:text="(" android:textSize="12pt" android:id="@+id/btnLParen" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text=")" android:textSize="12pt" android:id="@+id/btnRParen" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="," android:textSize="12pt" android:id="@+id/btnComma" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="B" android:textSize="12pt" android:id="@+id/btnBack" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
    </TableRow>
    <TableRow android:id="@+id/tableRow3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="0.166" android:weightSum="1">
        <Button android:text="7" android:textSize="12pt" android:id="@+id/btn7" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="8" android:textSize="12pt" android:id="@+id/btn8" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="9" android:textSize="12pt" android:id="@+id/btn9" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="+" android:textSize="12pt" android:id="@+id/btnPlus" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
    </TableRow>
    <TableRow android:id="@+id/tableRow4" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="0.166" android:weightSum="1">
    	<Button android:text="4" android:textSize="12pt" android:id="@+id/btn4" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="5" android:textSize="12pt" android:id="@+id/btn5" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="6" android:textSize="12pt" android:id="@+id/btn6" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="-" android:textSize="12pt" android:id="@+id/btnMinus" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
    </TableRow>
    <TableRow android:id="@+id/tableRow4" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="0.166" android:weightSum="1">
    	<Button android:text="1" android:textSize="12pt" android:id="@+id/btn1" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="2" android:textSize="12pt" android:id="@+id/btn2" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="3" android:textSize="12pt" android:id="@+id/btn3" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="*" android:textSize="12pt" android:id="@+id/btnTime" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
    </TableRow>
    <TableRow android:id="@+id/tableRow4" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="0.166" android:weightSum="1">
    	<Button android:text="." android:textSize="12pt" android:id="@+id/btnDot" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="0" android:textSize="12pt" android:id="@+id/btn0" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="=" android:textSize="12pt" android:id="@+id/btnEqual" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="/" android:textSize="12pt" android:id="@+id/btnDivide" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
    </TableRow>
</TableLayout>
<TableLayout
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:id="@+id/tableLayout1"
	android:layout_weight="0.1">
	<TableRow android:id="@+id/tableRow4" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="0.166" android:weightSum="1">
    	<Button android:text="Exp" android:textSize="12pt" android:id="@+id/btnExp" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="Ans" android:textSize="12pt" android:id="@+id/btnAns" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="Fun" android:textSize="12pt" android:id="@+id/btnFun" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
        <Button android:text="Var" android:textSize="12pt" android:id="@+id/btnVar" android:layout_width="wrap_content" android:layout_height="fill_parent" android:layout_weight="0.25"></Button>
    </TableRow>
</TableLayout>
</LinearLayout>
